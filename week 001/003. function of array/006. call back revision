function fun1 ()
{
  console.log("this is function 1");
}

function fun2 ()
{
  console.log("this is function 2");
}

function fntoAny( fn)                                  //// we can call any function.
{ 
  fn();
}

fntoAny(fun1);                        


//output
// this is function 1

///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////















//////////////////////////////////////////////////////// version  - 2 //////////////////////////////////////////////////////////////////////////////////////////////////////
////////////////////////////////////////////////// // if function take arguments ///////////////////////////////////////////////////////////////////////


















function fun1(str) {
  console.log(str);
}

function fun2(str2) {
  console.log(str2);
}

function fntoAny(fn) {
  fn();
}

fntoAny(() => fun1("hello, class what's"));  
                                                              // Using an arrow function to pass the function as an argument


//output
// hello, class what's









