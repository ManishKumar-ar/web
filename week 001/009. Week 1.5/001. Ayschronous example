function sum() {
  let a = 0;
  for (let i = 0; i < 10000; i++) {
    a = a + i;
  }

  return a;
}
function sum3() {
  let a = 10;
  for (let i = 0; i < 10000; i++) {
    a = a + i;
  }

  return a;
}

function sum4() {
  let a = 20;
  for (let i = 0; i < 10000; i++) {
    a = a + i;
  }

  return a;
}
function sum2() {
  let x = sum();
  console.log(x);
}

setTimeout(sum2, 10);

console.log("Hello");
let x = sum3();
console.log(x);

let y = sum4();
console.log(y);

//output
// Hello
// 49995010
// 49995020
// 49995000

