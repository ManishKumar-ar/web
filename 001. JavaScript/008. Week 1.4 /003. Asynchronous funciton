

function cube( n)
{
  return n*n*n;
}

function displayresult(a, b, fn ) {

  const val1 = fn(a);
  const val2 = fn(b);
   return val1 + val2;
  
}

const x = displayresult(2,4, function square(n){                                                // here we make asynchronous funciton
  return n*n
});
console.log(x);

//output
//20








////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////





function displayresult(a, b, fn ) {

  const val1 = fn(a);
  const val2 = fn(b);
   return val1 + val2;
  
}

const x = displayresult(2,4, function cube(n){
  return n*n*n;
});
console.log(x);

//output
//72
















